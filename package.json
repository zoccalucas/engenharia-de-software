{
  "name": "ac1-eng-software",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "staging": "sucrase-node src/main/server.ts",
    "production": "sucrase-node src/main/server.ts",
    "build": "tsc",
    "docker-build": "docker-compose build",
    "docker-push": "docker-compose push",
    "docker-start-dev": "docker-compose up -d app-dev",
    "docker-start-staging": "docker-compose up -d app-staging",
    "docker-start-production": "docker-compose up -d app-production",
    "lint": "eslint . --ext js,ts,tsx",
    "pre-commit": "npm run lint",
    "test": "jest --passWithNoTests --silent --noStackTrace",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@eduzz/eslint-config-houston": "^1.5.0",
    "@shelf/jest-mongodb": "^2.0.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/mongodb": "^3.1.7",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-plugin": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "npm-check-updates": "16.10.1",
    "patch-package": "^6.5.1",
    "sucrase": "^3.32.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ttab": "0.8.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "mongodb": "^3.1.1",
    "validator": "^13.9.0"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
